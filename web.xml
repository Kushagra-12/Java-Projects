<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOperator</servlet-name>
        <servlet-class>CheckOperator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Votin</servlet-name>
        <servlet-class>Votin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchVoter</servlet-name>
        <servlet-class>FetchVoter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OperatorES</servlet-name>
        <servlet-class>OperatorES</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOperator</servlet-name>
        <url-pattern>/CheckOperator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Votin</servlet-name>
        <url-pattern>/Votin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchVoter</servlet-name>
        <url-pattern>/FetchVoter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OperatorES</servlet-name>
        <url-pattern>/OperatorES</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
